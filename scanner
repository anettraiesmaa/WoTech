import java.util.Scanner;

    public class Main {

    public static void main(String[] args) {

        Scanner scan = new Scanner(System.in);

        System.out.println("Write something:");
        String s = scan.next();
        System.out.println("Write something again:");
        String myString = scan.next();

        System.out.println("Write a decimal:");
        double d = scan.nextDouble();
        System.out.println("Write a decimal again:");
        double myDouble = scan.nextDouble();
        scan.nextLine();

        System.out.println("Write a whole number:");
        int i = scan.nextInt();
        System.out.println("Write a whole number again:");
        int myInt = scan.nextInt();
        scan.nextLine();

        System.out.println("myString: " + s);
        System.out.println("myDouble: " + d);
        System.out.println("myInt: " + i);

        scan.close();

    }
}
